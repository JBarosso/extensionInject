{
    "manifest_version": 3,
    "name": "Injector Pro",
    "version": "1.0.0",
    "description": "Premium CSS and JS injection tool with Sidepanel interface.",
    "permissions": [
        "storage",
        "scripting",
        "activeTab",
        "sidePanel"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "side_panel": {
        "default_path": "sidepanel.html"
    },
    "action": {
        "default_title": "Open Injector Pro"
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}